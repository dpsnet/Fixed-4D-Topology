  "main_results": [
    {
      "label": "Theorem A",
      "name": "Fractal Weyl Law for Kleinian Groups",
      "statement": "For a geometrically finite Kleinian group Γ with limit set of Hausdorff dimension δ, the heat kernel trace satisfies: Θ_Γ(t) = Vol/(4πt)^(3/2) + c(δ)·t^(-(1+δ)/2) + O(t^(-1/2))"
    },
    {
      "label": "Theorem B", 
      "name": "p-adic Bowen Formula",
      "statement": "For a hyperbolic p-adic rational map φ, the Hausdorff dimension of the Julia set equals s* where P(-s*·log|φ'|_p) = 0"
    },
    {
      "label": "Corollary C",
      "name": "Unified Dimension Formula",
      "statement": "Both settings satisfy the pressure-dimension principle: dim_H is the unique value where an appropriate pressure functional vanishes"
    }
  ],
  
